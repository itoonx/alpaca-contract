{
    "extends": "solhint:recommended",
    "plugins": [],
    "rules": {
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "indent": false,
      "state-visibility": true,
      "two-lines-top-level-separator": false,
      "max-line-length": false,
      "not-rely-on-time": false,
      "const-name-snakecase": false,
      "separate-by-one-line-in-contract": false
    }
  }